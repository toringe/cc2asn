{
  "version": "2.0",
  "app_name": "cc2asn-rir-dl",
  "tags": {
    "Project": "cc2asn"
  },
  "environment_variables": {
    "TMPDIR": "/tmp/cc2asn",
    "BUCKET": "cc2asn-data",
    "REGION": "eu-west-1",
    "ARIN": "ftp://ftp.arin.net/pub/stats/arin/delegated-arin-extended-latest",
    "RIPE": "ftp://ftp.ripe.net/ripe/stats/delegated-ripencc-extended-latest",
    "AFRINIC": "ftp://ftp.afrinic.net/pub/stats/afrinic/delegated-afrinic-extended-latest",
    "APNIC": "ftp://ftp.apnic.net/pub/stats/apnic/delegated-apnic-extended-latest",
    "LACNIC": "ftp://ftp.lacnic.net/pub/stats/lacnic/delegated-lacnic-extended-latest"
  },
  "manage_iam_role": false,
  "iam_role_arn": "arn:aws:iam::719411478741:role/CC2ASN-Lambda-Execution",
  "stages": {
    "dev": {
      "environment_variables": {
        "LOGLEVEL": "debug",
        "STAGE": "dev",
        "RIRSNS": "arn:aws:sns:eu-west-1:719411478741:cc2asn-rir-dl-dev",
        "SEFSNS": "arn:aws:sns:eu-west-1:719411478741:cc2asn-sef-file-dev"
      }
    },
    "prod": {
      "environment_variables": {
        "LOGLEVEL": "warning",
        "STAGE": "prod",
        "RIRSNS": "arn:aws:sns:eu-west-1:719411478741:cc2asn-rir-dl-prod",
        "SEFSNS": "arn:aws:sns:eu-west-1:719411478741:cc2asn-sef-file-prod"
      }
    }
  }
}